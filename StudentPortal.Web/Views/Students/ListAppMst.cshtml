@{
}
@model StudentPortal.Web.Models.AppMstViewModel

<h1>Records Through Table</h1>
<table class="table">
    <thead>
        <tr>
            <th>Id</th>
            <th>Regno</th>
            <th>Name</th>
            <th>Email</th>
            <th>Phone</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var student in Model.FromTable)
        {
            <tr>
                <td>@student.Id</td>
                <td>@student.RegNo</td>
                <td>@student.Name</td>
                <td>@student.Email</td>
                <td>@student.MobileNo</td>
            </tr>
        }
    </tbody>
</table>

